{
  "name": "beelection",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "(sleep 5 ; wget localhost:3000/ping )& next start",
    "lint": "next lint",
    "deploy": "fly m run . -p 443:3000/tcp:tls:http -p 80:3000/tcp:http --region fra -a beelection",
    "kill": "fly machine destroy --force $(fly machine list --app beelection --json | jq -j '.[] | .id')"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.5"
  }
}
